{
  "expo": {
    "name": "jobbi-frontend",
    "slug": "jobbi-frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/img/icon.webp",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/img/splash-icon.webp",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/img/adaptive-icon.webp",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./src/assets/img/favicon.webp"
    }
  }
}
